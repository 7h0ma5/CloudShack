{
  "name": "cloudshack",
  "version": "0.0.1",
  "main": "public/js/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "angular": "latest",
    "angular-route": "latest",
    "angular-resource": "latest",
    "angular-animate": "latest",
    "angular-cookies": "latest",
    "ng-file-upload": "latest",
    "normalize-css": "latest",
    "font-awesome": "latest",
    "socket.io-client": "latest",
    "angular-hotkeys": "chieffancypants/angular-hotkeys#latest"
  },
  "overrides": {
    "socket.io-client": {
      "main": "socket.io.js"
    },
    "font-awesome": {
      "main": [
        "fonts",
        "css/font-awesome.css"
      ]
    },
    "angular-hotkeys": {
        "main": "build/hotkeys.js"
    }
  }
}
