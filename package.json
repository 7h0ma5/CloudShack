{
  "name": "cloudshack",
  "version": "0.2.4",
  "description": "Amateur Radio logbook server",
  "author": "Thomas Gatzweiler <dl2ic@darc.de>",
  "license": "GPL-3.0",
  "main": "server.js",
  "bin": {
    "cloudshack": "./server.js"
  },
  "files": [
    "controllers",
    "lib",
    "public",
    "server.js",
    "package.json",
    "LICENSE"
  ],
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git://github.com/7h0ma5/cloudshack.git"
  },
  "scripts": {
    "start": "node server.js",
    "build": "npm install && bower install && gulp",
    "prepublish": "bower install && gulp",
    "test": "mocha test/",
    "test-coverage": "istanbul cover node_modules/mocha/bin/_mocha -- test/",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- test/",
    "doc": "jsdoc -p -d docs -r lib/",
    "recess": "recess --noOverqualifying false webapp/app.less"
  },
  "dependencies": {
    "adif": "^0.2.3",
    "async": "^0.9.0",
    "body-parser": "^1.11.0",
    "dateformat": "^1.0.11",
    "errorhandler": "^1.3.3",
    "express": "^4.11.2",
    "libxmljs": "^0.13.0",
    "lodash": "^3.2.0",
    "nano": "^6.0.2",
    "serialport": "^1.5.0",
    "serve-static": "^1.8.1",
    "socket.io": "^1.3.3",
    "superagent": "^0.21.0",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "gulp": "^3.8.11",
    "gulp-changed": "^1.1.1",
    "gulp-concat": "^2.4.3",
    "gulp-less": "^3.0.0",
    "gulp-livereload": "^3.7.0",
    "gulp-load-plugins": "^0.8.0",
    "gulp-minify-css": "^0.4.5",
    "gulp-ng-annotate": "^0.5.2",
    "gulp-order": "^1.1.1",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "istanbul": "^0.3.5",
    "jsdoc": "^3.3.0-beta1",
    "mocha": "^2.1.0",
    "recess": "^1.1.9",
    "supertest": "^0.15.0"
  }
}
